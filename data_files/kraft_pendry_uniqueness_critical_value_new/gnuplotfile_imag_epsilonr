set output "critical_zeta_vs_imag_epsilon_pendry_uniqueness.eps"
#set grid
set yrange [-0.0:1.2]
set xrange [-0.1:-0.5]
set xlabel 'Im({/Symbol e}_r)'
#set ylabel '|{/Symbol z}_0|'
plot 'outputfile_real_epsilon_-1' u 2:3 w l  t 'Re({/Symbol e}_r) = - 1.0', 'outputfile_real_epsilon_-2' u 2:3 w l  t 'Re({/Symbol e}_r) = - 2.0','outputfile_real_epsilon_-3' u 2:3 w l  t 'Re({/Symbol e}_r) = - 3.0', 'outputfile_real_epsilon_-4' u 2:3 w l  t 'Re({/Symbol e}_r) = - 4.0'
